{
    "name": "phundament/app",
    "description": "12factor Application Template for Yii 2.0 Framework",
    "keywords": [
        "phundament",
        "yii2",
        "app",
        "framework",
        "application",
        "template",
        "boilerplate",
        "vagrant",
        "docker",
        "fig",
        "12factor",
        "dotenv"
    ],
    "homepage": "http://www.phundament.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Tobias Munk",
            "email": "schmunk@usrbin.de",
            "homepage": "http://www.phundament.com"
        }
    ],
    "support": {
        "issues": "https://github.com/phundament/app/issues?state=open",
        "source": "https://github.com/phundament/app"
    },
    "repositories": [
            {
                "type": "vcs",
                "url": "https://github.com/open-ecommerce/yii2-backuprestore"
            }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "dektrium/yii2-user": "0.9.*",
        "dmstr/yii2-adminlte-asset": "2.*",
        "dmstr/yii2-app-command": "@stable",
        "dmstr/yii2-log": "@stable",
        "dmstr/yii2-migrate-command": "@stable",
        "mikehaertl/php-shellcommand": "1.*",
        "vlucas/phpdotenv": "1.1.*",
        "yiisoft/yii2": "2.0.*",
        "kartik-v/yii2-dynagrid": "@dev",
        "yiisoft/yii2-bootstrap": "2.*",
        "yiisoft/yii2-composer": "*",
        "yiisoft/yii2-swiftmailer": "2.*",
        "kartik-v/yii2-grid": "*",
        "kartik-v/yii2-widget-select2": "*",
        "kartik-v/yii2-mpdf": "*",
        "kartik-v/yii2-helpers": "dev-master",
        "kartik-v/yii2-widgets": "*",
        "kartik-v/yii2-datecontrol": "dev-master",
        "kartik-v/yii2-export": "dev-master",
        "spanjeta/yii2-backup": "*",
        "open-ecommerce/yii2-backuprestore": "dev-master",
        "bower-asset/font-awesome": "^4.5",
        "bower-asset/motion-ui": "^1.2.2",
        "bower-asset/what-input": "^1.1",
        "bower-asset/admin-lte": "*"
    },
    "require-dev": {
        "yiisoft/yii2-apidoc": "2.*",
        "yiisoft/yii2-codeception": "2.*",
        "yiisoft/yii2-coding-standards": "2.*",
        "yiisoft/yii2-debug": "2.*",
        "yiisoft/yii2-faker": "2.*",
        "yiisoft/yii2-gii": "2.*",
        "schmunk42/yii2-giiant": "*",
        "cebe/markdown-latex": "dev-master"
    },
    "scripts": {
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "web/assets": "0777",
                    "yii": "0755"
                }
            ]
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "config": {
        "process-timeout": 1800
    }
}
